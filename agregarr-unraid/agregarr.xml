<?xml version="1.0"?>
<Container version="2">
  <Name>agregarr</Name>
  <Repository>https://github.com/untraceablez/unraid-apps</Repository>
  <Registry>https://hub.docker.com/r/agregarr/agregarr</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/untraceablez/unraid-apps/issues</Support>
  <Project>https://github.com/agregarr/agregarr</Project>
  <Overview>Agregarr is an automated Plex collection management tool that regularly updates your Plex collections with content from multiple sources. It integrates with your existing media management tools to provide seamless collection automation</Overview>
  <Category>MediaApp:Video MediaServer:Video Status:Stable</Category>
  <WebUI>http://[IP]:[PORT]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/untraceablez/unraid-apps/main/agregarr-unraid/agregarr.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1762363460</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Application Configuration Path" Target="/app/config" Default="/mnt/user/appdata/agreggar/config" Mode="rw" Description="The default application configuration path. (/app/config in container)" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/agreggar/config</Config>
  <Config Name="WebUI Port" Target="7171" Default="7171" Mode="tcp" Description="Default WebUI port. Usually 7171" Type="Port" Display="always" Required="true" Mask="false">7171</Config>
  <TailscaleStateDir/>
</Container>